module StringTest {
  fn test_concat() {
    let res = "Hello " ++ "World!"
    OwlUnit:assert_eq(res, "Hello World!")
    OwlUnit:refute_eq(res, "something else")

    let many_res = "One " ++ "and " ++ "Two"
    OwlUnit:assert_eq(many_res, "One and Two")
  }

  fn main() {
    OwlUnit:assert_eq("ab", "ab")
    OwlUnit:refute_eq("one", "two")

    test_concat()
  }
}
