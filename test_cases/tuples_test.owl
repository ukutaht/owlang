module tuples_test {
  fn two_and_four() {
    (2, 4)
  }

  fn nested() {
    ((1, 2), (3, 4))
  }

  fn main() {
    owl_unit:assert_eq((1, 2), (1, 2))
    owl_unit:assert_eq((1, (2, (3, 4))), (1, (2, (3, 4))))

    owl_unit:assert_eq(tuple:nth(two_and_four(), 0), 2)
    owl_unit:assert_eq(tuple:nth(two_and_four(), 1), 4)
    owl_unit:assert_eq(tuple:nth(tuple_nth(nested(), 1), 0), 3)
  }
}
